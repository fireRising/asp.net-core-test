@model ClassifProject.Models.EducationLevel
@{
    ViewData["Title"] = "Delete Classifier";
}
<div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Code</th>
                <th scope="col">Name</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td scope="row">@Model.id_edulvl</td>
                <td>@Model.code_edulvl</td>
                <td>@Model.name_edulvl</td>

            </tr>
        </tbody>
    </table>
    <div>
        <form asp-controller="Home" asp-action="DeleteEduLvl" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Удалить" />
            </div>
        </form>
    </div>
</div>
