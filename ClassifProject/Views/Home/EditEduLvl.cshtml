@model ClassifProject.Models.EducationLevel
@{
    ViewData["Title"] = "Edit Education Level";
}
<form asp-action="EditEduLvl" asp-controller="Home" asp-route-id="@Model.id_edulvl">
    <div class="form-group">
        <label asp-for="id_edulvl" class="control-label">Id</label>
        <input type="text" asp-for="id_edulvl" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="code_edulvl" class="control-label">Код</label>
        <input type="text" asp-for="code_edulvl" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="name_edulvl" class="control-label">Название</label>
        <input type="text" asp-for="name_edulvl" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Send" class="btn btn-default" />
    </div>
</form>